  },
  "browserslist": {
    "production": [
      "last 1 version",
      "> 1%",
      "node 8",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "xo": {
    "extends": [
      "xo-react"
    ],
    "plugins": [
      "emotion"
    ],
    "space": true,
    "prettier": true,
    "parser": "babel-eslint",
    "rules": {
      "emotion/jsx-import": "error",
      "emotion/no-vanilla": "error",
      "emotion/import-from-emotion": "error",
      "emotion/styled-import": "error",
      "react/jsx-pascal-case": [
        "error",
        {
          "allowAllCaps": true
        }
      ]
    },
    "overrides": [
      {
        "files": "components/**/*.js",
        "envs": [
          "browser"
        ]
      },
      {
        "files": "models/*.js",
        "rules": {
          "unicorn/filename-case": [
            "error",
            {
              "case": "pascalCase"
            }
          ]
        }
      }
    ]
  },
  "eslint": {
    "settings": {
      "react": {
        "version": "16.7"
      }
    }
  },
  "lint-staged": {
    "*.{js}": [
      "npx xo --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
