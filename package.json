  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "xo": {
    "space": true,
    "prettier": true,
    "rules": {
      "unicorn/string-content": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts",
          "**/*.tsx"
        ],
        "extends": [
          "xo-react"
        ]
      },
      {
        "files": "src/**/*.tsx",
        "envs": [
          "browser"
        ],
        "rules": {
          "import/no-unassigned-import": [
            "error",
            {
              "allow": [
                "**/*.css"
              ]
            }
          ],
          "react/jsx-no-useless-fragment": [
            "off"
          ]
        }
      }
    ]
  },
  "eslint": {
    "settings": {
      "react": {
        "version": "16.12"
      }
    }
  },
  "lint-staged": {
    "*.{js}": [
      "npx xo --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
