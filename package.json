  "browserslist": {
    "production": [
      "last 1 version",
      "> 1%",
      "node 8",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "xo": {
    "space": true,
    "prettier": true,
    "extends": [
      "xo-typescript",
      "xo-react"
    ],
    "extensions": [
      "ts",
      "tsx",
      "js"
    ],
    "rules": {
      "@typescript-eslint/indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ]
    },
    "overrides": [
      {
        "files": "components/**/*.tsx",
        "envs": [
          "browser"
        ]
      }
    ]
  },
  "eslint": {
    "settings": {
      "react": {
        "version": "16.12"
      }
    }
  },
  "lint-staged": {
    "*.{js}": [
      "npx xo --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
